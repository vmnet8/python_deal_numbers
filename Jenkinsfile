pipeline {

    agent any
    
    stages {
    
        stage("build") {
            
            steps {
                echo "will run python script"
            }
        stage("test") {
            
            steps {
                echo "will test  python script"
            }
        stage("deploy") {
            
            steps {
                echo "will deploy  python script"
            }
        }
    }
}
